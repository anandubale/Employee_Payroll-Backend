{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {

        "no-console": 0,
    "no-unused-vars": "error",
    "quotes": [
      "error",
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "semi-style": ["error", "last"],
    "max-len": [
      "error",
      {
        "code": 80
      }
    ],
    "no-irregular-whitespace": "error",
    "no-trailing-spaces": "error",
    "no-multi-spaces": "error",
    "eqeqeq": ["error", "always"]
    }
}
